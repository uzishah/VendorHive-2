{
  "name": "vendorhive",
  "version": "1.0.0",
  "description": "VendorHive - A marketplace for service providers",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bash ./run.sh",
    "dev:frontend": "bash ./start-frontend.sh",
    "dev:backend": "bash ./start-backend.sh",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm run start"
  },
  "keywords": [
    "marketplace",
    "vendors",
    "services",
    "mern",
    "typescript"
  ],
  "author": "",
  "license": "MIT"
}